initiate
    total_opened: int := 0
on open(filename: str)
    total_opened: int := @total_size[0] + 1
output open(filename, total_opened)

on close(filename: str)
    total_opened: int := @total_opened[0] - 1
output open(filename)

on write(filename: str, data: str, size: double)
    size: double := data.length
    sha256: str := sha256(data)
output write(filename, size, sha256)



