#!/bin/bash

echo " ==== RESULT ==== " >> result-dejavu-prediction-bits-compare

echo "########## experiment 1 ##########"
echo "########## experiment 1 ##########" >> result-dejavu-prediction-bits-compare
(gtime scala -J-Xmx16g -cp .:dejavu.jar TraceMonitorSpec1.scala --logfile trace_1_minimum.csv --bits 5 --prediction 3 --prediction_type smart) >> result-dejavu-prediction-bits-compare 2>&1

