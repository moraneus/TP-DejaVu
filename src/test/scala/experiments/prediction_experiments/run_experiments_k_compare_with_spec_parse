#!/bin/bash

echo "=== RESULT ===" >> result-dejavu-prediction

echo "########## spec 1 ##########" >> result-dejavu-prediction

(gtime ./dejavu --specfile=spec1.qtl --logfile=trace_1_minimum.csv --bits=5 --prediction=3 --prediction_type=smart) >> result-dejavu-prediction 2>&1
(gtime ./dejavu --specfile=spec1.qtl --logfile=trace_1_minimum.csv --bits=5 --prediction=3 --prediction_type=brute) >> result-dejavu-prediction 2>&1
(gtime ./dejavu --specfile=spec1.qtl --logfile=trace_1_minimum.csv --bits=5 --prediction=5 --prediction_type=smart) >> result-dejavu-prediction 2>&1
(gtime ./dejavu --specfile=spec1.qtl --logfile=trace_1_minimum.csv --bits=5 --prediction=5 --prediction_type=brute) >> result-dejavu-prediction 2>&1

(gtime ./dejavu --specfile=spec1.qtl --logfile=trace_1_medium.csv --bits=5 --prediction=3 --prediction_type=smart) >> result-dejavu-prediction 2>&1
(gtime ./dejavu --specfile=spec1.qtl --logfile=trace_1_medium.csv --bits=5 --prediction=3 --prediction_type=brute) >> result-dejavu-prediction 2>&1
(gtime ./dejavu --specfile=spec1.qtl --logfile=trace_1_medium.csv --bits=5 --prediction=5 --prediction_type=smart) >> result-dejavu-prediction 2>&1
(gtime ./dejavu --specfile=spec1.qtl --logfile=trace_1_medium.csv --bits=5 --prediction=5 --prediction_type=brute) >> result-dejavu-prediction 2>&1

(gtime ./dejavu --specfile=spec1.qtl --logfile=trace_1_maximum.csv --bits=5 --prediction=3 --prediction_type=smart) >> result-dejavu-prediction 2>&1
(gtime ./dejavu --specfile=spec1.qtl --logfile=trace_1_maximum.csv --bits=5 --prediction=3 --prediction_type=brute) >> result-dejavu-prediction 2>&1
(gtime ./dejavu --specfile=spec1.qtl --logfile=trace_1_maximum.csv --bits=5 --prediction=5 --prediction_type=smart) >> result-dejavu-prediction 2>&1
(gtime ./dejavu --specfile=spec1.qtl --logfile=trace_1_maximum.csv --bits=5 --prediction=5 --prediction_type=brute) >> result-dejavu-prediction 2>&1


echo "########## spec 2 ##########" >> result-dejavu-prediction

