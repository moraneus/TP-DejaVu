#!/bin/bash

echo "=== RESULT ===" >> result-dejavu-prediction

echo "########## spec 1 ##########" >> result-dejavu-prediction

(gtime ./dejavu --specfile=spec1.qtl --logfile=trace_spec_1_order_1.csv --bits=20 --prediction=3 --prediction_type=smart) >> result-dejavu-prediction 2>&1
(gtime ./dejavu --specfile=spec1.qtl --logfile=trace_spec_1_order_1.csv --bits=20 --prediction=3 --prediction_type=brute) >> result-dejavu-prediction 2>&1
(gtime ./dejavu --specfile=spec1.qtl --logfile=trace_spec_1_order_1.csv --bits=20 --prediction=5 --prediction_type=smart) >> result-dejavu-prediction 2>&1
(gtime ./dejavu --specfile=spec1.qtl --logfile=trace_spec_1_order_1.csv --bits=20 --prediction=5 --prediction_type=brute) >> result-dejavu-prediction 2>&1

(gtime ./dejavu --specfile=spec1.qtl --logfile=trace_spec_1_order_2.csv --bits=20 --prediction=3 --prediction_type=smart) >> result-dejavu-prediction 2>&1
(gtime ./dejavu --specfile=spec1.qtl --logfile=trace_spec_1_order_2.csv --bits=20 --prediction=3 --prediction_type=brute) >> result-dejavu-prediction 2>&1
(gtime ./dejavu --specfile=spec1.qtl --logfile=trace_spec_1_order_2.csv --bits=20 --prediction=5 --prediction_type=smart) >> result-dejavu-prediction 2>&1
(gtime ./dejavu --specfile=spec1.qtl --logfile=trace_spec_1_order_2.csv --bits=20 --prediction=5 --prediction_type=brute) >> result-dejavu-prediction 2>&1

(gtime ./dejavu --specfile=spec1.qtl --logfile=trace_spec_1_order_3.csv --bits=20 --prediction=3 --prediction_type=smart) >> result-dejavu-prediction 2>&1
(gtime ./dejavu --specfile=spec1.qtl --logfile=trace_spec_1_order_3.csv --bits=20 --prediction=3 --prediction_type=brute) >> result-dejavu-prediction 2>&1
(gtime ./dejavu --specfile=spec1.qtl --logfile=trace_spec_1_order_3.csv --bits=20 --prediction=5 --prediction_type=smart) >> result-dejavu-prediction 2>&1
(gtime ./dejavu --specfile=spec1.qtl --logfile=trace_spec_1_order_3.csv --bits=20 --prediction=5 --prediction_type=brute) >> result-dejavu-prediction 2>&1


