(gtime ./dejavu --specfile=spec2.qtl --logfile=trace_2_maximum.csv --bits=5 --prediction=5 --prediction_type=smart) >> result-dejavu-prediction 2>&1
(gtime ./dejavu --specfile=spec2.qtl --logfile=trace_2_maximum.csv --bits=5 --prediction=5 --prediction_type=brute) >> result-dejavu-prediction 2>&1

echo "########## spec 3 ##########" >> result-dejavu-prediction

(gtime ./dejavu --specfile=spec3.qtl --logfile=trace_3_minimum.csv --bits=5 --prediction=3 --prediction_type=smart) >> result-dejavu-prediction 2>&1
